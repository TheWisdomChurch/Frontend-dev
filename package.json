{
  "name": "WisdomHouseHq-WepApplication",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 2000",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix --max-warnings 0",
    "type-check": "tsc --noEmit",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "validate": "npm run type-check && npm run lint && npm run prettier",
    "pre-push-check": "npm run type-check && npm run lint && npm run prettier && npm run build",
    "prepare": "husky install",
    "docker:dev": "docker-compose up app-dev",
    "docker:prod": "docker-compose up app-prod",
    "docker:build": "docker build -t wisdom-house .",
    "docker:build:dev": "docker build -t wisdom-house-dev -f Dockerfile.dev .",
    "docker:run": "docker run -p 3000:3000 wisdom-house",
    "docker:run:dev": "docker run -p 3000:3000 -v $(pwd):/app -v /app/node_modules -v /app/.next -e NODE_ENV=development wisdom-house-dev",
    "docker:down": "docker-compose down"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-brands-svg-icons": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@reduxjs/toolkit": "^2.10.1",
    "@types/gsap": "^1.20.2",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "framer-motion": "^12.23.24",
    "googleapis": "^165.0.0",
    "gsap": "^3.13.0",
    "lucide-react": "^0.545.0",
    "next": "15.5.5",
    "qr-code-styling": "^1.9.2",
    "react": "19.1.0",
    "react-day-picker": "^9.11.1",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.66.0",
    "react-qr-code": "^2.0.18",
    "react-redux": "^9.2.0",
    "react-toastify": "^11.0.5",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.37.0",
    "eslint-config-next": "15.5.5",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.4",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.0",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md,mdx,css,html,yml,yaml}": [
      "prettier --write"
    ]
  }
}
